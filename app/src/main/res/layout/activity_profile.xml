                    android:layout_marginBottom="-1dp"
                    android:layout_gravity="top|left"
                    android:alpha="0.699999988079071"
                    android:gravity="right|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
