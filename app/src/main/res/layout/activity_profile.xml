        android:layout_marginTop="205dp"
        android:layout_marginLeft="40dp"
        android:layout_width="330dp"
        android:layout_height="140dp"
        android:background="@drawable/container_progress_tab">

        <ImageView
            android:id="@+id/image_background"
            android:layout_width="328dp"
            android:layout_height="140dp"
            android:layout_gravity="top|left"
            android:alpha="0.50"
            android:scaleType="centerInside"
            android:src="@drawable/image_background"/>

        <LinearLayout
            android:id="@+id/container_stats_tab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="50dp"
            android:layout_gravity="top|left"
            android:background="@drawable/container_stats_tab"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingBottom="4dp"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:paddingTop="4dp">

            <FrameLayout
                android:id="@+id/container_distance"
                android:layout_width="76dp"
                android:layout_height="60dp">

                <TextView
                    android:id="@+id/text_km"
                    android:layout_width="wrap_content"
                    android:layout_height="15dp"
                    android:layout_marginLeft="56dp"
                    android:layout_marginTop="38dp"
                    android:layout_marginBottom="-1dp"
                    android:layout_gravity="top|left"
                    android:alpha="0.699999988079071"
                    android:gravity="right|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="km"
                    android:textColor="#FF333333"
                    android:textSize="11dp" />

                <TextView
                    android:id="@+id/text_distance_value"
                    android:layout_width="wrap_content"
                    android:layout_height="22dp"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="15.5dp"
                    android:layout_marginBottom="-0.5dp"
                    android:layout_gravity="top|left"
                    android:gravity="right|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="103,2"
                    android:textColor="#FF333333"
                    android:textSize="17dp" />

                <FrameLayout
                    android:id="@+id/container_stats_icon_distance"
                    android:layout_width="33dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="-16dp"
                    android:layout_marginTop="14dp"
                    android:layout_gravity="top|left">

                    <ImageView
                        android:id="@+id/image_stats_icon_distance"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="-4dp"
                        android:layout_gravity="top|left"
                        android:scaleType="centerInside"
                        android:src="@drawable/image_stats_icon_distance" />
                </FrameLayout>
            </FrameLayout>

            <ImageView
                android:id="@+id/image_divider1"
                android:layout_width="1dp"
                android:layout_height="44dp"
                android:layout_marginLeft="8dp"
                android:alpha="0.10000000149011612"
                android:background="@drawable/image_divider1"
                android:scaleType="centerInside" />

            <FrameLayout
                android:id="@+id/container_time"
                android:layout_width="80dp"
                android:layout_height="60dp"
                android:layout_marginLeft="8dp">

                <TextView
                    android:id="@+id/text_hr"
                    android:layout_width="wrap_content"
                    android:layout_height="15dp"
                    android:layout_marginLeft="65dp"
                    android:layout_marginTop="38dp"
                    android:layout_marginBottom="-1dp"
                    android:layout_gravity="top|left"
                    android:alpha="0.699999988079071"
                    android:gravity="right|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
