                android:src="@drawable/drawable_image_10" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_marginLeft="48dp"
                android:layout_marginTop="15.5dp"
                android:layout_marginRight="168dp"
